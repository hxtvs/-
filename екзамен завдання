# Функція для введення матриці
def input_matrix(name, rows=4, cols=5):
    print(f"Введіть матрицю {name} ({rows} рядки по {cols} чисел):")
    matrix = []
    for i in range(rows):
        row = list(map(float, input(f"Рядок {i+1}: ").split()))
        matrix.append(row)
    return matrix

# Функція для знаходження індексу рядка з максимальним елементом
def max_row_index(matrix):
    max_val = max(max(row) for row in matrix)
    for i, row in enumerate(matrix):
        if max_val in row:
            return i

# Функція для обміну рядків між двома матрицями
def swap_max_rows(A, B):
    row_A = max_row_index(A)
    row_B = max_row_index(B)
    A[row_A], B[row_B] = B[row_B], A[row_A]

# Функція для виведення матриці
def print_matrix(matrix, name):
    print(f"Матриця {name}:")
    for row in matrix:
        print(row)

# Основна програма
def main():
    A = input_matrix("A")
    B = input_matrix("B")
    swap_max_rows(A, B)
    print_matrix(A, "A після обміну")
    print_matrix(B, "B після обміну")

# Виклик основної програми
main()
